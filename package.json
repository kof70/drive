{
  "name": "local-collaborative-workspace",
  "version": "1.0.0",
  "description": "Un espace de travail collaboratif local multiplateforme sans d√©pendance Internet",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server/index.ts",
    "dev:client": "vite",
    "build": "pnpm build:server && pnpm build:client",
    "build:server": "node esbuild.server.js --production",
    "build:server:dev": "node esbuild.server.js",
    "build:client": "vite build",
    "start": "node dist/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:manual": "ts-node scripts/test-manual.ts",
    "test:file-upload": "ts-node scripts/test-file-upload.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "build:extension": "node esbuild.extension.js --production",
    "build:extension:dev": "node esbuild.extension.js",
    "watch:extension": "node esbuild.extension.js --watch",
    "build:all": "pnpm build && pnpm build:extension",
    "copy:extension-assets": "node scripts/copy-extension-assets.js",
    "package:extension": "pnpm build:all && pnpm copy:extension-assets && node scripts/package-vsix.js",
    "package:extension:quick": "pnpm build:extension && pnpm copy:extension-assets && node scripts/package-vsix.js"
  },
  "packageManager": "pnpm@8.0.0",
  "keywords": [
    "collaborative",
    "workspace",
    "local",
    "real-time",
    "canvas",
    "file-sharing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "better-sqlite3": "^12.4.1",
    "bonjour-service": "^1.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "lucide-react": "^0.554.0",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "sqlite3": "^5.1.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-fetch": "^2.6.13",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vscode/vsce": "^2.22.0",
    "concurrently": "^8.2.2",
    "electron-rebuild": "^3.2.9",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "form-data": "^4.0.4",
    "jest": "^29.7.0",
    "node-fetch": "2",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^7.1.11"
  }
}
